{
  "main": "main.js",
  "scripts": {
    "test": "npm run prettier:check",
    "prestart": "browserify --transform browserify-css frontend/main.js | terser > frontend/main.bundle.js",
    "start": "node . --nickname $NICKNAME --skin $SKIN",
    "prettier:check": "prettier --check '**/*'",
    "prettier:format": "prettier --write '**/*'"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-ws": "4.0.0",
    "slitherode": "8.0.1",
    "ws": "7.0.0",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "browserify": "16.2.3",
    "browserify-css": "0.15.0",
    "husky": "2.4.0",
    "material-components-web": "2.3.0",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0",
    "terser": "4.0.0"
  }
}
