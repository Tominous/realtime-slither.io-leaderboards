{
  "main": "main.js",
  "scripts": {
    "test": "npm run prettier:check",
    "prestart": "if [ -n \"$PROJECT_REMIX_CHAIN\" ]; then npm run browserify:all; else npm run browserify:frontend; fi",
    "start": "node .",
    "browserify:all": "npm run browserify:frontend && npm run browserify:glitch-tools",
    "browserify:frontend": "browserify --transform browserify-css frontend/main.js | terser > frontend/main.bundle.js",
    "browserify:glitch-tools": "browserify glitch-tools/browser/follow-bot.js | terser > glitch-tools/browser/follow-bot.bundle.js",
    "prettier:check": "prettier --check '**/*'",
    "prettier:format": "prettier --write '**/*'"
  },
  "dependencies": {
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-ws": "4.0.0",
    "ip-port-regex": "2.0.0",
    "ow": "0.13.2",
    "pretty-bytes": "5.2.0",
    "slitherode": "8.0.1",
    "ws": "7.0.0",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "browserify": "16.2.3",
    "browserify-css": "0.15.0",
    "husky": "2.4.0",
    "material-components-web": "2.3.0",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0",
    "terser": "4.0.0"
  }
}
